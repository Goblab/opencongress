<%= render :partial => 'heading' %>
<div id="views">
    <%= render :partial => 'sidebar' %>
    <div id="contain_issues" class="contain_list">
        <div id="sort_nav">
            <%= render :partial => 'filter_issues' %>
            <%= will_paginate @subjects if @subjects.respond_to? :total_pages %>
        </div>
        <%= render :partial => 'issue_list', :locals => {:subjects => @subjects} %>
        <%= will_paginate @subjects if @subjects.respond_to? :total_pages %>
    </div>
</div>
